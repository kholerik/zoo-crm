{
    "name": "bupy7/yii2-datetime-converter",
    "description": "Converting date and/or time from saving format to display and converse.",
    "type": "yii2-extension",
    "version": "1.1.1",
    "keywords": [
        "yii2",
        "extension",
        "date",
        "carbon",
        "formatter",
        "datetime",
        "translator",
        "time",
        "datetime",
        "converter"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "1.0.1",
        "nesbot/carbon": "*"
    },
    "autoload": {
        "psr-4": {
            "bupy7\\datetime\\converter\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
